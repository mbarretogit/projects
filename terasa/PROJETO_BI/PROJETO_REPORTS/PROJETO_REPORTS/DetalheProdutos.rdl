  <DataSources>
    <DataSource Name="DS_PRODUTOS">
      <DataSourceReference>DS_DW</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>8153d540-3efc-47b1-b2b4-3db01b292796</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="SET_PRODUTOS">
      <Query>
        <DataSourceName>DS_PRODUTOS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FORNECEDOR">
            <Value>=Parameters!FORNECEDOR.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT P.NOME AS PRODUTO,
	C.NOME AS CATEGORIA
FROM DIM_PRODUTO P
JOIN FATO FA ON P.IDSK = FA.IDPRODUTO
JOIN DIM_FORNECEDOR F ON F.IDSK = FA.IDFORNECEDOR
JOIN CATEGORIA C ON C.IDCATEGORIA = P.ID_CATEGORIA
WHERE F.NOME = @FORNECEDOR</CommandText>
      </Query>
      <Fields>
        <Field Name="PRODUTO">
          <DataField>PRODUTO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CATEGORIA">
          <DataField>CATEGORIA</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SET_FORNECEDOR">
      <Query>
        <DataSourceName>DS_PRODUTOS</DataSourceName>
        <CommandText>SELECT DISTINCT F.NOME AS FORNECEDOR
FROM DIM_FORNECEDOR F
JOIN FATO FT ON FT.IDFORNECEDOR = F.IDSK
WHERE F.FIM IS NULL
ORDER BY FORNECEDOR</CommandText>
      </Query>
      <Fields>
        <Field Name="FORNECEDOR">
          <DataField>FORNECEDOR</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
