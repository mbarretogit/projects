  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DS_DW</DataSourceName>
        <CommandText>SELECT        DIM_CLIENTE.REGIAO, T.ANO, F.LUCRO_TOTAL AS LUCRO, F.QUANTIDADE AS QTD, F.TOTAL_ITEM AS TOTAL, F.CUSTO_TOTAL AS CUSTO
FROM            DIM_CLIENTE INNER JOIN
                         FATO AS F ON DIM_CLIENTE.IDSK = F.IDCLIENTE INNER JOIN
                         DIM_TEMPO AS T ON T.IDSK = F.IDTEMPO
ORDER BY DIM_CLIENTE.REGIAO, T.ANO</CommandText>
      </Query>
      <Fields>
        <Field Name="REGIAO">
